<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="io.nzo.booth.model">
	<class name="Board" table="gz_board">
		<id name="boardId" column="board_id" />
		<!--  --><property name="id" column="id" />
		<property name="tableNumber" column="table_number" />
		<property name="title" column="title" />
		<property name="headerContent" column="header_content" />
		<property name="footerContent" column="footer_content" />
		<property name="memo" column="memo" />
		<property name="postCount" column="post_count" />
		<property name="commentCount" column="comment_count" />
		<property name="permission" column="permission" />
		<property name="creationTime" column="creation_time" />
	</class>
</hibernate-mapping>
